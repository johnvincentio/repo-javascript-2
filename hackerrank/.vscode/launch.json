  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.js"
    },
    {
      "name": "climbing-the-leaderboard",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "args": [
        "src/medium/climbing-the-leaderboard/test/**/*.js",
        "--no-timeouts"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "env": { "NODE_ENV": "testing" }
    },
    {
      "name": "form-a-magic-square",
      "type": "node",
      "request": "launch",
